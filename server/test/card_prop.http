### Список всех свойств товара id=1
GET /api/card/6/property/getall HTTP/1.1
Host: localhost:5000

### Получить одно свойство товара
GET /api/card/6/property/getone/1 HTTP/1.1
Host: localhost:5000

### Создать новое свойство товара
POST /api/card/1/property/create HTTP/1.1
Host: localhost:5000
Content-type: application/json; charset=utf-8

{
    "name": "Новое свойство товара",
    "description": "описание товара"
}

### Обновить свойство товара
PUT /api/card/1/property/update/3 HTTP/1.1
Host: localhost:5000
Content-type: application/json; charset=utf-8

{
    "name": "Свойство товара (updated)",
    "desc": "описание товара обновлено"
}

### Удалить свойство товара
DELETE /api/card/1/property/delete/3 HTTP/1.1
Host: localhost:5000